{
  "name": "hardhat-project",
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.1",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "chai": "^4.2.0",
    "eslint": "^7.18.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "ethereum-waffle": "^3.2.1",
    "ethers": "^5.0.24",
    "hardhat": "^2.0.6",
    "husky": "^4.3.8",
    "hardhat-gas-reporter": "^1.0.4",
    "prettier": "^2.2.1",
    "prettier-plugin-solidity": "^1.0.0-beta.3",
    "solhint": "^3.3.2",
    "solhint-plugin-prettier": "0.0.5"
  },
  "scripts": {
    "lint": "solhint contracts/**/*.sol",
    "prettier": "prettier --write contracts/**/*.sol",
    "test": "hardhat test ./test/orca.js",
    "testSandbox": "hardhat --config ./hardhat.config.gnosisTest.js test ./test/gnosis.js  ./test/genericFunctions",
    "testAll": "hardhat --config ./hardhat.config.gnosisTest.js test",
    "compile": " hardhat compile"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier && npm run lint && npm run test"
    }
  },
  "dependencies": {
    "@openzeppelin/contracts": "^3.3.0",
    "solc": "^0.7.4"
  }
}
